<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ovopark.mapper.InspectionAuditReasonMapper" >
  <resultMap id="BaseResultMap" type="com.ovopark.po.InspectionAuditReason" >
